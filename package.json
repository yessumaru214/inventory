{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=webpack.mix.js",
        "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=webpack.mix.js",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.mix.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=webpack.mix.js"
    },
    "devDependencies": {
        "axios": "^1.7.9",
        "bootstrap-sass": "^3.3.7",
        "cross-env": "^5.2.1",
        "inflight": "^1.0.6",
        "jquery": "^3.2",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.4",
        "rimraf": "^6.0.1",
        "webpack-cli": "^6.0.0"
    },
    "dependencies": {
        "@types/webpack": "^5.28.5",
        "ajv": "^6.12.6",
        "sweetalert2": "^7.32.4",
        "vue": "^2.7.16",
        "vue-moment": "^4.0.0",
        "vue-multiselect": "^2.1.4",
        "vuejs-datepicker": "^1.5.4"
    }
}
